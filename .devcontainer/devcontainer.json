{
	"name": "next-app-devcontainer",
	"dockerComposeFile": [
		"compose.yml",
		"../docker/dev/compose.yml"
	],
	"service": "next_app",
	"workspaceFolder": "/workspace",
	"runServices": [
		"postgres"
	],
	"customizations": {
		"vscode": {
			"extensions": [
				"ritwickdey.LiveServer",
				"esbenp.prettier-vscode",
				"dbaeumer.vscode-eslint",
				"oderwat.indent-rainbow",
				"ms-azuretools.vscode-docker",
				"donjayamanne.githistory",
				"mhutchie.git-graph"
			]
		}
	},
	"initializeCommand": "docker network inspect next_app_network || docker network create next_app_network"
}